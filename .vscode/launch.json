    "version": "0.2.0",
    "configurations": [
        { 
             "name": "(gdb) Attach",
            "type": "cppdbg",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "program": "${workspaceFolder}/compiled_code/kernel8.elf",
            "MIMode": "gdb",
            "miDebuggerPath": "/home/tsrapnik/projects/compilers/gcc-linaro-7.1.1-2017.08-x86_64_aarch64-elf/bin/aarch64-elf-gdb",
            "miDebuggerServerAddress": "localhost:3333",
            // "setupCommands": [
            //     {
            //         "description": "Enable pretty-printing for gdb",
            //         "text": "-enable-pretty-printing",
            //         "ignoreFailures": true
            //     }
            // ]
        },
        {
            "name": "(gdb) Pipe Launch",
            "type": "cppdbg",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "program": "${workspaceFolder}/compiled_code/kernel8.elf",
            "MIMode": "gdb",
            "miDebuggerPath": "/home/tsrapnik/projects/compilers/gcc-linaro-7.1.1-2017.08-x86_64_aarch64-elf/bin/aarch64-elf-gdb",
            "miDebuggerServerAddress": "localhost:3333",
            "stopAtEntry": false,
            "environment": [],
            "externalConsole": true,
            "pipeTransport": {
                "pipeProgram": "openocd",
                "pipeArgs": ["-f", "c232hm.cfg", "-f", "rpi3.cfg"],
                "pipeCwd": "${workspaceFolder}"
            },
        },
    ]
}